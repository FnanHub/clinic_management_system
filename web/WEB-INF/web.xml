<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">



    <servlet>
        <servlet-name>clinicService</servlet-name>
        <servlet-class>controller.ClinicalServiceController</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>clinicService</servlet-name>
        <url-pattern>/clinicService</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>about</servlet-name>
        <servlet-class>controller.About</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>about</servlet-name>
        <url-pattern>/about</url-pattern>
    </servlet-mapping>


    <servlet>
        <servlet-name>adminlogin</servlet-name>
        <servlet-class>controller.AdminLogIn</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>adminlogin</servlet-name>
        <url-pattern>/admin</url-pattern>
    </servlet-mapping>


    <servlet>
        <servlet-name>adminhome</servlet-name>
        <servlet-class>controller.AdminHomeController</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>adminhome</servlet-name>
        <url-pattern>/admin_home</url-pattern>
    </servlet-mapping>


</web-app>